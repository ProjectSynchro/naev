#: dat/landing.lua:243
#, lua-format
msgid ""
"\"I'll let you land for the modest price of %s credit.\"\n"
"\n"
"Pay %s credit?"
msgid_plural ""
"\"I'll let you land for the modest price of %s credits.\"\n"
"\n"
"Pay %s credits?"
msgstr[0] ""
"„Für den geringfügigen Preis von %s Credit lasse ich dich landen.“\n"
"\n"
"%s Credit zahlen?"
msgstr[1] ""
"„Für den geringfügigen Preis von %s Credits lasse ich dich landen.“\n"
"\n"
"%s Credits zahlen?"

#: dat/landing.lua:247
