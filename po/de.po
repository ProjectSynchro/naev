#: dat/missions/trader/trader_escort.lua:36
#, lua-format
msgid "Escort a medium convoy to %s in %s."
msgstr "Begleite einen mittelgroßen Konvoi nach %s in %s."

#: dat/missions/trader/trader_escort.lua:37
#, lua-format
