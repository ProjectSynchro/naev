#.
#. <?xml version='1.0' encoding='utf8'?>
#. <mission name="Dark Shadow">
#. <flags>
#. <unique />
#. </flags>
#. <avail>
#. <priority>3</priority>
#. <chance>100</chance>
#. <location>None</location>
