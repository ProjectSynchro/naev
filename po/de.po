#. Messages
#: dat/missions/neutral/pirbounty_dead.lua:102
#, lua-format
msgid "MISSION FAILURE! %s got away."
msgstr "MISSION FEHLGESCHLAGEN! %s konnte fliehen."

#: dat/missions/neutral/pirbounty_dead.lua:103
#, lua-format
msgid "MISSION FAILURE! Another pilot eliminated %s."
msgstr "MISSION FEHLGESCHLAGEN! Ein anderer Pilot hat %s eliminiert."

#: dat/missions/neutral/pirbounty_dead.lua:106
#: dat/missions/proteron/dissbounty_dead.lua:87
msgid "Bounty Hunt"
msgstr "Kopfgeldjagd"

#: dat/missions/neutral/pirbounty_dead.lua:109
#, lua-format
