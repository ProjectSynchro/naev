#.
#. <?xml version='1.0' encoding='utf8'?>
#. <mission name="Dvaered Ballet">
#. <flags>
#. <unique />
#. </flags>
#. <avail>
#. <priority>2</priority>
#. <chance>100</chance>
#. <location>None</location>
#. </avail>
