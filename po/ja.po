#: dat/outfits/biocore_brain/heavy_bioship_brain_stage_2.xml:10
#: dat/outfits/biocore_brain/heavy_bioship_brain_stage_3.xml:11
#: dat/outfits/biocore_brain/heavy_bioship_brain_stage_4.xml:11
#: dat/outfits/biocore_brain/heavy_bioship_brain_stage_5.xml:11
#: dat/outfits/biocore_brain/heavy_bioship_brain_stage_6.xml:11
#: dat/outfits/biocore_brain/light_bioship_brain_stage_1.xml:10
#: dat/outfits/biocore_brain/light_bioship_brain_stage_2.xml:11
#: dat/outfits/biocore_brain/light_bioship_brain_stage_3.xml:11
#: dat/outfits/biocore_brain/medium_bioship_brain_stage_1.xml:9
#: dat/outfits/biocore_brain/medium_bioship_brain_stage_2.xml:9
#: dat/outfits/biocore_brain/medium_bioship_brain_stage_3.xml:10
